programa {
  funcao inicio() {
    // ler numeros
    // declaração das variáveis
    inteiro n1, n2
    // entrada de dados
    escreva ("Digite um número: ")
    leia (n1)
    escreva ("Digite outro número: ")
    leia (n2)
    se (n1>n2){
      escreva ( "O maior número é: ", n1)
    }
    senao se (n1<n2) {
      escreva (" O maior numero é: " , n2)
    }
    senao se (n1==n2){
      escreva (" Os números são iguais")
    }


  }
}
