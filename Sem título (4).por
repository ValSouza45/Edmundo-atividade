programa {
  funcao inicio() {
    // declaração de variável

    inteiro kminicial, kmfinal
    real media, litros

    escreva (" Digite o Km inicial: ")
    leia (kminicial)
    escreva ("Digite o km final: ")
    leia(kmfinal)
    escreva ("Digite a quantidade de litros: ")
    leia (litros)

    media = litros / (kmfinal-kminicial)

    escreva (" a média de consumo é: ", media)

    
  }
}
